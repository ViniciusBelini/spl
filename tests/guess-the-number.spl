import io
import math

// Guess the number!
print("Guess the number!")

function start() <bool>
    <int> number := math.Rand(0, 100)
    while true
        <str> strGuess := io.Input("Choose a number between 0 and 100: ")
        if !math.IsInt(strGuess)
            print("It is not a valid number!")
            continue
        end
        <int> guess := int(strGuess)

        if guess > number
            print("Too high!")
        else if guess < number
            print("Too low")
        else if guess == number
            print("Congratulations! You won the game.")
            return true
        end
    end
end

start()
