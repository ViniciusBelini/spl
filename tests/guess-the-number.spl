import io
import math

// Guess the number!

io.Print("Guess the number!")

function start() <bool>
    <int> number := math.Rand(0, 100)
    while true
        <str> strGuess := io.Input("Choose a number between 0 and 100: ")
        <int> guess := int(strGuess)

        if guess > number
            io.Print("Too high!")
        else if guess < number
            io.Print("Too low")
        else if guess == number
            io.Print("Congratulations! You won the game.")
            return true
        end
    end
end

start()
